<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.mi.ur.de.urfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".start_activity"
            android:label="@string/Start_Activity_Label"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.mi.ur.de.urfit.MainActivity"/>
        </activity>

        <activity android:name=".ergebnis_activity"
            android:label="@string/Ergebnis_Activity_Label"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.mi.ur.de.urfit.MainActivity"/>
        </activity>

        <activity android:name=".profile_Activity"
            android:label="@string/profile_Activity_Label"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.mi.ur.de.urfit.MainActivity"/>
        </activity>

        <activity android:name=".hilfe_activity"
            android:label="@string/hilfe_Activity_Label"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.mi.ur.de.urfit.MainActivity"/>
        </activity>


    </application>

</manifest>